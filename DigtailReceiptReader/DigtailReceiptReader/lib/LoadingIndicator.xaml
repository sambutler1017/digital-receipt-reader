<?xml version="1.0" encoding="UTF-8"?>
<ContentView 
    x:Name="this"
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="DigtailReceiptReader.lib.LoadingIndicator">
    
    <ContentView.Content>
        <ActivityIndicator BindingContext="{x:Reference this}" IsRunning="{Binding Enabled, FallbackValue=True}" Color="{Binding LoadingColor, FallbackValue=Orange}"/>
    </ContentView.Content>
</ContentView>